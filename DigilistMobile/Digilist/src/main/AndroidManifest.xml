<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.sena.digilistmobile.digilist">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher2"
        android:label="@string/app_name"
        android:theme="@style/Theme.Tema">
        <activity
            android:name="co.edu.sena.digilistmobile.digilist.Login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="co.edu.sena.digilistmobile.digilist.Menu_Inicial_Al"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name="co.edu.sena.digilistmobile.digilist.Inicio" />
        <activity android:name="co.edu.sena.digilistmobile.digilist.Graficas" />
        <activity android:name="co.edu.sena.digilistmobile.digilist.Administrador" />
        <activity
            android:name="co.edu.sena.digilistmobile.digilist.Menu_Inicial_Ad"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
    </application>

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>
